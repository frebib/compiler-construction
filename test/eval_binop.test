/% CODE %/

// Test some binary operations
function evalBinop() {
  x = 1 * 3;
  n = 4 / ~++x;
  y = ~x + ~n;
  return ~y <= 5;
}

/% TREE %/

Program [
  ("evalBinop", [], Seq [
    Asg (
      Identifier "x",
      BinaryOp (
        Times,
        Const 1,
        Const 3
      )
    );
    Asg (
      Identifier "n",
      BinaryOp (
        Divide,
        Const 4,
        Deref (
          UnaryOp (PreInc, Identifier "x")
        )
      )
    );
    Asg (
      Identifier "y",
      BinaryOp (
        Plus,
        Deref (Identifier "x"),
        Deref (Identifier "n")
      )
    );
    BinaryOp (
      Leq,
      Deref (Identifier "y"),
      Const 5
    )
  ])
]

/% RESULT %/

[
  Boolean true
]

/% END %/

/% TEST %/

function sequences() {
  callme(one, two, three, four, five);
  callmeagain(a, b, c, 1, 2, 3);
  onestopshop(noope);
}

/% OUTPUT %/

Program [
  ("sequences", [],
    (* A sequence of function calls *)
    Seq (
      (* A sequence of parameters *)
      Application (Identifier "callme",
        Seq (Identifier "one",
          Seq (Identifier "two",
            Seq (Identifier "three",
              Seq (Identifier "four",
                Identifier "five")
            )
          )
        )
      ), Seq (
        Application (
          Identifier "callmeagain",
          Seq (Identifier "a",
            Seq (Identifier "b",
              Seq (Identifier "c",
                Seq (Const 1,
                  Seq (Const 2,
                    Const 3)
                )
              )
            )
          )
        ),
        Application (
          Identifier "onestopshop",
          (* A single element as only one param supplied *)
          Identifier "noope"
        )
      )
    )
  )
]

/% END %/

/% CODE %/

function evalIfBool() {
  g = true;
  s = false;
  if (~g) {
    x = !~g;
    p = ~x && ~s || true;
  } else {
    p = false;
  }
  return ~p;
}

/% TREE %/

Program [
  ("evalIfBool", [], Seq (
    Asg (Identifier "g", Boolean true),
    Seq (
      Asg (Identifier "s", Boolean false),
      Seq (
        If (Deref (Identifier "g"), Seq (
          Asg (
            Identifier "x",
            UnaryOp (Not, Deref (Identifier "g"))
          ),
          Asg (
            Identifier "p",
            BinaryOp (Or,
              BinaryOp (And,
                Deref (Identifier "x"),
                Deref (Identifier "s")
              ),
              Boolean true
            )
          )),
          Asg (Identifier "p", Boolean false)
        ), 
        Deref (Identifier "p")
      )
    )
  ))
]

/% RESULT %/

[
  Boolean true
]

/% END %/

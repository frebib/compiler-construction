/% CODE %/

function application() {
  return a(b(c(fish(), d(1, abc, xyz(1)))));
}

/% TREE %/

Program [
  ("application", [],
    Application (
      Identifier "a",
      Application (
        Identifier "b",
        Application (
          Identifier "c",
          Seq [
            Application (Identifier "fish", Empty);
            Application (Identifier "d", 
              Seq [
                Const 1;
                Identifier "abc";
                Application (Identifier "xyz", Const 1)
              ]
            )
          ]
        )
      )
    )
  )
]

/% RESULT %/

[
  Empty
]

/% END %/
